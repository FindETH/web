    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby": "^2.31.1",
    "gatsby-plugin-canonical-urls": "^2.9.0",
    "gatsby-plugin-create-client-paths": "^2.10.0",
    "gatsby-plugin-env-variables": "^2.0.0",
    "gatsby-plugin-layout": "^1.9.0",
    "gatsby-plugin-manifest": "^2.11.0",
