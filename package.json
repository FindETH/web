    "@typescript-eslint/parser": "^3.2.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-gatsby": "^0.4.9",
    "codecov": "^3.7.1",
    "cross-env": "^6.0.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.21.2",
