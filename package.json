    "gatsby-plugin-s3": "^0.3.3",
    "gatsby-plugin-sitemap": "^2.4.5",
    "gatsby-plugin-styled-components": "^3.3.14",
    "gatsby-plugin-ts-config": "^0.2.2",
    "gatsby-plugin-typescript": "^2.4.21",
    "gatsby-plugin-typescript-checker": "^1.1.1",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.10",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
