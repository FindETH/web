{
  "presets": [
    ["babel-preset-gatsby", {
      "reactRuntime": "automatic"
    }],
    "@babel/preset-typescript",
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "chrome": "78",
          "firefox": "70"
        }
      }
    ]
  ],
  "plugins": ["@babel/plugin-syntax-bigint"],
  "env": {
    "production": {
      "plugins": [
        [
          "@babel/plugin-transform-runtime",
          {
            "corejs": 3,
            "version": "^7.10.1",
            "useESModules": true
          }
        ]
      ]
    }
  }
}
